{
    "name": "qubus/cache",
    "type": "library",
    "description": "Database abstraction layer.",
    "keywords": [
        "cache",
        "psr",
        "psr-16",
        "psr-6",
        "simple-cache",
        "caching"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "joshua@joshuaparker.dev"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/cache": "^3.0",
        "psr/simple-cache": "^3.0",
        "qubus/config": "^2.0",
        "qubus/exception": "^2.0",
        "qubus/filesystem": "^2.0",
        "qubus/support": "^2.3"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\Cache\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubus\\Tests\\Cache\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "provide": {
        "psr/cache-implementation": "^3.0",
        "psr/simple-cache-implementation": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
